{
  "name": "@aigniter/website-holdings",
  "version": "0.0.1",
  "scripts": {
    "dev": "next --port 8080",
    "build": "next build",
    "start": "next start",
    "build-stats": "ANALYZE=true yarn run build",
    "export": "next export -o dist",
    "build-prod": "run-s clean build export",
    "clean": "rimraf .next dist",
    "svgr": "svgr --typescript --ignore-existing --out-dir src/assets/images public/assets/images",
    "svgr:overwrite": "svgr --typescript --out-dir src/assets/images public/assets/images",
    "gen:theme-typings": "chakra-cli tokens src/theme/index.ts",
    "format": "prettier --write \"./**/*.{ts,js,json,md}\" --ignore-path ../../.prettierignore",
    "lint": "eslint . --ignore-path ../../.eslintignore --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint --fix . --ignore-path ../../.eslintignore --ext ts --ext tsx --ext js --ext jsx",
    "typecheck": "tsc -p tsconfig.json  --noEmit",
    "postinstall": "yarn run gen:theme-typings"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.3.3",
    "@chakra-ui/theme": "^1.6.2",
    "@chakra-ui/theme-tools": "^1.0.4",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "@twind/aspect-ratio": "^0.1.1",
    "@twind/typography": "^0.0.1",
    "framer-motion": "^3.3.0",
    "next": "^10.0.4",
    "next-seo": "^4.20.0",
    "next-svgr": "^0.0.2",
    "next-tinacms-github": "^0.37.0",
    "next-tinacms-json": "^0.37.0",
    "next-tinacms-markdown": "^0.37.0",
    "postcss-preset-env": "^6.7.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-tinacms-date": "^0.37.0",
    "react-tinacms-editor": "^0.37.0",
    "react-tinacms-field-condition": "^0.2.5",
    "react-tinacms-github": "^0.37.0",
    "react-tinacms-inline": "^0.37.0",
    "styled-components": "^5.2.1",
    "styled-jsx-plugin-postcss": "^4.0.0",
    "tailwindcss": "^2.0.3",
    "tinacms": "^0.37.0",
    "twind": "^0.14.6"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^1.2.1",
    "@next/bundle-analyzer": "^10.0.6",
    "@svgr/cli": "^5.5.0",
    "@types/node": "^14.14.19",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.14.2",
    "autoprefixer": "^10.1.0",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.2",
    "postcss-flexbugs-fixes": "^5.0.2",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "license": "ISC",
  "private": "true"
}
